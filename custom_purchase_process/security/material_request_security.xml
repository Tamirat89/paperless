<odoo>
    <!-- Create Custom Category -->
    <record id="material_request_category" model="ir.module.category">
        <field name="name">Store Requestion</field>
        <field name="sequence" eval="11"/>
    </record>
    <!-- Readonly Group -->
    <!-- <record id="group_material_request_readonly" model="res.groups">
        <field name="name">Readonly</field>
        <field name="category_id" ref="material_request_category"/>
    </record> -->

    <!-- User Group -->
    <record id="group_material_request_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="material_request_category"/>      
        
    </record>

    <!-- Approver Group -->
    <record id="group_material_request_approver" model="res.groups">
        <field name="name">Approver</field>
        <field name="category_id" ref="material_request_category"/>
        <!-- <field name="implied_ids" eval="[
            (4, ref('custom_purchase_process.group_material_request_user'))
        ]"/> -->
    </record>

    <!-- Store Manager Group -->
    <record id="group_material_request_store_manager" model="res.groups">
        <field name="name">Store Manager</field>
        <field name="category_id" ref="material_request_category"/>
        <!-- <field name="implied_ids" eval="[
            (4, ref('custom_purchase_process.group_material_request_approver'))
        ]"/> -->
    </record>


    <record id="group_material_request_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="material_request_category"/>
        <!-- <field name="implied_ids" eval="[
            (4, ref('custom_purchase_process.group_material_request_store_manager'))
        ]"/> -->
    </record>
</odoo>
















